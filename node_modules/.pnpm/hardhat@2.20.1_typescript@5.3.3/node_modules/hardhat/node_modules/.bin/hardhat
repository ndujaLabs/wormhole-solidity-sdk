#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules/hardhat/internal/cli/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules/hardhat/internal/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules/hardhat/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules/hardhat/internal/cli/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules/hardhat/internal/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules/hardhat/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/hardhat@2.20.1_typescript@5.3.3/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../../internal/cli/bootstrap.js" "$@"
fi
