#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ensdomains+ens@0.4.5_@ensdomains+resolver@0.2.4_@ethersproject+abi@5._d3identibz4ivildp4vbmosl2i/node_modules/ethereum-waffle/bin/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ensdomains+ens@0.4.5_@ensdomains+resolver@0.2.4_@ethersproject+abi@5._d3identibz4ivildp4vbmosl2i/node_modules/ethereum-waffle/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ensdomains+ens@0.4.5_@ensdomains+resolver@0.2.4_@ethersproject+abi@5._d3identibz4ivildp4vbmosl2i/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ensdomains+ens@0.4.5_@ensdomains+resolver@0.2.4_@ethersproject+abi@5._d3identibz4ivildp4vbmosl2i/node_modules/ethereum-waffle/bin/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ensdomains+ens@0.4.5_@ensdomains+resolver@0.2.4_@ethersproject+abi@5._d3identibz4ivildp4vbmosl2i/node_modules/ethereum-waffle/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ensdomains+ens@0.4.5_@ensdomains+resolver@0.2.4_@ethersproject+abi@5._d3identibz4ivildp4vbmosl2i/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/waffle" "$@"
else
  exec node  "$basedir/../../bin/waffle" "$@"
fi
