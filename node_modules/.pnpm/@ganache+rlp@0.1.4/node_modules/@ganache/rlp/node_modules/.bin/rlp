#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/rlp@2.2.6/node_modules/rlp/bin/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/rlp@2.2.6/node_modules/rlp/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/rlp@2.2.6/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/rlp@2.2.6/node_modules/rlp/bin/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/rlp@2.2.6/node_modules/rlp/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/rlp@2.2.6/node_modules:/Users/francescosullo/Projects/ndujaLabs/wormhole-solidity-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../rlp@2.2.6/node_modules/rlp/bin/rlp" "$@"
else
  exec node  "$basedir/../../../../../../rlp@2.2.6/node_modules/rlp/bin/rlp" "$@"
fi
